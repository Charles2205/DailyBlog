

<%- include('../partials/header',{
    title:'Clean Blog',
    discription:'A Blog Theme by Start Bootstrap',
    img:'home-bg.jpg',
    style:'site-heading',
    disStyle:'subheading',
}) %>

<div class="container px-4 px-lg-5">
    <div class="row gx-4 gx-lg-5 justify-content-center">
        <div class="col-md-10 col-lg-8 col-xl-7">
         
            
            <% records.forEach((record)=>{ %>
                
                <div class="post-preview">
                    <a href="post.html">
                        <h2 class="post-title"> <%= record.title %> </h2>
                        <h3 class="post-subtitle"> <%= record.description %> </h3>
                    </a>
                    <p class="post-meta">
                        Posted by
                        <a href="#!"> <%= record.author %> </a>
                        
                    </p>
                </div>
                <!-- Divider-->
                <hr class="my-4" />


            <% }) %>


        </div>
    
    </div>
</div>
<!-- Main Content-->



<%-contentFor('title') %>
Home